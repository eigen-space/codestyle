{
    "name": "eslint-plugin-eigenspace-script",
    "version": "0.1.4",
    "description": "Eigenspace team's codestyle plugin for script linting",
    "license": "MIT",
    "scripts": {
        "build": "yarn copy:assets && yarn compile",
        "compile": "node ./node_modules/typescript/bin/tsc",
        "copy:assets": "node ./dev/scripts/copy.js"
    },
    "dependencies": {
        "@typescript-eslint/eslint-plugin": "4.18.0",
        "@typescript-eslint/experimental-utils": "4.18.0",
        "@typescript-eslint/parser": "4.18.0",
        "eslint-plugin-unicorn": "9.1.1",
        "eslint-utils": "1.4.2"
    },
    "devDependencies": {
        "@eigenspace/helper-scripts": "1.0.81",
        "@types/eslint": "4.16.8",
        "@types/node": "10.12.21",
        "typescript": "3.8.2"
    },
    "peerDependencies": {
        "eslint": "7.x"
    }
}